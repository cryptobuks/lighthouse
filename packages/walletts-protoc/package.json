{
  "name": "wallet-protoc",
  "version": "1.0.0",
  "description": "protoc files and generated typescript library",
  "main": "index.ts",
  "author": "joemphilips",
  "license": "MIT",
  "dependencies": {
    "protoc": "^1.0.3"
  },
  "scripts": {
    "test": "npm run build",
    "build": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:generated --ts_out=service=true:generated -I ./proto proto/*.proto"
  },
  "devDependencies": {
    "ts-protoc-gen": "^0.4.0"
  }
}
